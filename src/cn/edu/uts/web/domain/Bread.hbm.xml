<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.edu.uts.web.domain.Bread">
		<id name="id">
			<generator class="increment"></generator>
		</id>
		<property name="name" length="45"></property>
		<property name="type" length="10"></property>
		<property name="cost"></property>
		<property name="day"></property>
		<property name="deadline"></property>
		<property name="description"></property>
		<property name="code"></property>
		<property name="filename"></property>
		<property name="path"></property>
	</class>
</hibernate-mapping>